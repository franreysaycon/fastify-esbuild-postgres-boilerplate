{
  "name": "fastify-esbuild-postgres-boilerplate",
  "version": "0.1.3",
  "description": "A fastify boilerplate built with esbuild and batteries incldued docker setup with postgres.",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "dev": "tsx watch src",
    "build": "rm -rf build && tsx build.ts",
    "start": "npm run build && node build",
    "lint": "lint-staged",
    "test": "jest",
    "commit": "cz",
    "release": "standard-version --no-verify",
    "release:patch": "standard-version --no-verify --release-as patch"
  },
  "keywords": [
    "fastify",
    "esbuild",
    "postgres",
    "typescript"
  ],
  "author": "Franrey Anthony S. Saycon",
  "license": "MIT",
  "dependencies": {
    "@fastify/autoload": "^5.4.1",
    "@fastify/cors": "8.1.1",
    "@fastify/swagger": "^8.1.0",
    "@fastify/swagger-ui": "^1.1.0",
    "dotenv": "16.0.3",
    "fastify": "4.9.2",
    "fastify-plugin": "4.3.0",
    "fs-recursive": "1.1.7",
    "joi": "17.7.0",
    "knex": "2.3.0",
    "pg": "8.8.0",
    "pg-hstore": "2.3.4"
  },
  "devDependencies": {
    "@commitlint/cli": "17.2.0",
    "@commitlint/config-conventional": "17.2.0",
    "@commitlint/cz-commitlint": "17.2.0",
    "@types/cors": "2.8.10",
    "@types/jest": "29.2.0",
    "@types/node": "18.11.8",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "commitizen": "4.2.5",
    "esbuild": "0.15.12",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.1",
    "jest": "29.2.2",
    "lint-staged": "13.0.3",
    "nodemon": "2.0.7",
    "prettier": "2.7.1",
    "standard-version": "9.5.0",
    "tiny-glob": "0.2.9",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "tsx": "3.11.0",
    "typescript": "4.8.4"
  }
}
